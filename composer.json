{
  "name": "mothership-ec/up",
  "description": "Up! A composer library for self-updating projects",
  "keywords": [
    "composer",
    "update",
    "installer"
  ],
  "homepage": "http://mothership.ec",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "The Mothership Development Team",
      "email": "dev@mothership.ec"
    }
  ],
  "support": {
    "issues": "http://github.com/mothership-ec/up/issues"
  },
  "require": {
    "php": ">=5.4",
    "mothership-ec/composer": "dev-feature/update-composer as 1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": {
      "Mothership\\Up\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mothership\\Up\\Test\\": "tests"
    }
  }
}
